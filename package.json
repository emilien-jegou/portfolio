{
  "name": "portfolio",
  "description": "A qwik.js portfolio",
  "engines": {
    "node": ">=12.0.0"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "wrangler pages publish ./dist",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "dev.ts": "concurrently -n server,tsc -c magenta,cyan \"npm run dev\" \"npm run ts-watch\"",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "lint.fix": "npm run lint -- --fix",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist",
    "start": "vite --open --mode ssr",
    "test.unit": "vitest",
    "test.unit.ui": "vitest --ui",
    "ts-watch": "tsc --noEmit --incremental --preserveWatchOutput --pretty --watch",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik-city": "^1.14.1",
    "@cloudflare/workers-types": "^4.20250617.0",
    "@modular-forms/qwik": "^0.29.1",
    "@qwikdev/pwa": "^0.0.4",
    "@svgx/vite-plugin-qwik": "^1.0.1",
    "@tailwindcss/vite": "^4.1.10",
    "@types/eslint": "^9.6.1",
    "@types/js-cookie": "^3.0.6",
    "@types/matter-js": "^0.19.8",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitest/ui": "^3.2.3",
    "concurrently": "^9.1.2",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-qwik": "^1.14.1",
    "eslint-plugin-sonarjs": "^3.0.3",
    "eslint-plugin-unicorn": "^59.0.1",
    "gray-matter": "^4.0.3",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "rehype-autolink-headings": "^7.1.0",
    "remark": "^15.0.1",
    "tailwindcss": "^4.1.10",
    "ts-essentials": "^10.1.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "undici": "*",
    "vite": "^5.2.10",
    "vite-imagetools": "^7.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3",
    "wrangler": "^4.20.0"
  },
  "dependencies": {
    "@builder.io/qwik": "^1.14.1",
    "@onwo/icons": "^0.0.12-alpha",
    "@onwo/primitives": "^0.0.12-alpha",
    "@onwo/tailwindcss": "^0.0.12-alpha",
    "@onwo/ui": "^0.0.12-alpha",
    "@qwik-ui/headless": "^0.6.8",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "js-cookie": "^3.0.5",
    "matter-js": "^0.20.0",
    "npm-check-updates": "^18.0.1",
    "remeda": "^2.23.0",
    "sharp": "^0.34.2",
    "tailwind-merge": "^3.3.1",
    "ts-pattern": "^5.7.1"
  }
}
